<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="theme-color" content="#142F4C">
    <meta name="msapplication-navbutton-color" content="#142F4C">
    <meta name="apple-mobile-web-app-status-bar-style" content="#142F4C">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>UploadBroBot</title>
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500" rel="stylesheet">
    <link href="/assets/css/style.css" rel="stylesheet">
    <link href="/assets/js/app.css" rel="stylesheet">
    <link rel="icon" href="/favicon.ico">
</head>
<body>
<div id="app">
</div>

<script src="/assets/js/vanilla-tilt/vanilla-tilt.min.js"></script>
<script>
    setTimeout(function () {
        VanillaTilt.init(document.querySelectorAll(".your-element"), {
            reverse: false,  // reverse the tilt direction
            max: 35,     // max tilt rotation (degrees)
            perspective: 1000,   // Transform perspective, the lower the more extreme the tilt gets.
            scale: 1,      // 2 = 200%, 1.5 = 150%, etc..
            speed: 300,    // Speed of the enter/exit transition
            transition: true,   // Set a transition on enter/exit.
            easing: "cubic-bezier(.03,.98,.52,.99)",
        });
    }, 1000);
</script>
<script src="/assets/js/app.js"></script>
</body>
</html>